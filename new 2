class Entry {

	int clientID;	// will get from client
	Date date;
	String facility;
	String entry;
	
	static class Date(String month, int day, int year) {
		this.month = month;
		this.day = day;
		this.year = year;
	}
	
	public Entry(int clientID, Date date, String facility, String entry) {
		this.clientID = clientID;
		this.date = date;
		this.facility = facility;
		this.entry = entry;
	}
	
	



}